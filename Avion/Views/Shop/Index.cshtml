            <div class="row">
                <div class="col-md-3">
                    <div class="shopFilters">
                        <h5>Product Types</h5>
                        <ul>
                            <li><input type="checkbox" name="" id="" />Furniture</li>
                            <li><input type="checkbox" name="" id="" />Homeware</li>
                            <li><input type="checkbox" name="" id="" />Sofas</li>
                            <li><input type="checkbox" name="" id="" />Light fittings</li>
                            <li><input type="checkbox" name="" id="" />Accessories</li>
                        </ul>
                        <hr />
                        <h5>Price</h5>
                        <div class="card">
                            <div class="price-content">
                                <div>
                                    <label>Min</label>
                                    <p id="min-value">$50</p>
                                </div>

                                <div>
                                    <label>Max</label>
                                    <p id="max-value">$500</p>
                                </div>
                            </div>

                            <div class="range-slider">
                                <div class="range-fill"></div>

                                <input type="range"
                                       class="min-price"
                                       value="10"
                                       min="10"
                                       max="500"
                                       step="10" />
                                <input type="range"
                                       class="max-price"
                                       value="500"
                                       min="10"
                                       max="500"
                                       step="10" />
                            </div>
                        </div>
                        <a class="filter" href="">Filter</a>
                        <hr />

                        <h5>Brands</h5>
                        <ul>
                            <li><input type="checkbox" name="" id="" />Robert Smith</li>
                            <li><input type="checkbox" name="" id="" />Liam Gallagher</li>
                            <li><input type="checkbox" name="" id="" />Biggie Smalls</li>
                            <li><input type="checkbox" name="" id="" />Thom Yorke</li>
                        </ul>
                    </div>
                </div>
