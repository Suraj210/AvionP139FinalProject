@model List<PrivacyVM>
@{
    ViewData["Title"] = "Index";
}

@section css {
    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/assets/css/pages/privacy/privacy.css" />
}

<!-- Main Part -->
<main>
    <section id="privacy">
        <div class="container">
            <div class="privacyContainer">
                <h1>Privacy Policy</h1>
                <ul>
                    @foreach (var privacy in Model)
                    {
                        <li>
                            <h6>@privacy.Title</h6>
                            <p>
                                @privacy.Context
                            </p>
                        </li>

                    }
                  
                </ul>
            </div>
        </div>
    </section>
</main>

